        function show(){
            var val=$("#in").val();
//            alert(val);
            $.ajax({
                type:"post",
                dataType:"json",
                data:val,
                url:"test/"+val,
                success:function(text){
                    $("#choose option").remove();
                    $(text).each(function(i){
                        $("#choose").append("<option value='"+text[i].value+"'>"+text[i].name+"</option>");
                    });
